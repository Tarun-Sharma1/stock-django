{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",  // Adjust the build script file name accordingly
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build_files.sh"  // Adjust the build script file name accordingly
    }
  ],
  "env": {
    "PYENV_VERSION": "3.10.0"
    // Add other environment variables your application needs
  }
}
